
<div class="card">
  <div class="card-header">
    <a href="{{ url }}">
      <img src="{{feature_image}}" alt="" class="card-img-top">
    </a>
  </div>
  <div class="card-body">
    <div class="tags-area">
      <span href="{{url}}" class="text-danger">
        <b>
          {{tags}}
        </b>
      </span>
    </div>
    <a class="card-title" target="_blank" href="{{ url }}" >{{title}}</a>

  </div>
  <div class="card-footer">
    <a target="_blank" class="btn btn-outline-danger btn-round" href="{{ url }}">Read more</a>
  </div>
</div>
